{
  "name": "quak-vim",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build && npm run build:extension",
    "build:extension": "esbuild src/extension/*.ts --bundle --outdir=dist/browser",
    "watch": "ng build --watch --configuration development",
    "watch:extension": "esbuild src/extension/*.ts --bundle --watch --outdir=dist/browser",
    "build:watch": "concurrently \"npm run watch\" \"npm run watch:extension\"",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^18.2.0",
    "@angular/common": "^18.2.0",
    "@angular/compiler": "^18.2.0",
    "@angular/core": "^18.2.0",
    "@angular/forms": "^18.2.0",
    "@angular/platform-browser": "^18.2.0",
    "@angular/platform-browser-dynamic": "^18.2.0",
    "@angular/router": "^18.2.0",
    "@ngrx/operators": "^18.0.2",
    "@ngrx/signals": "^18.0.2",
    "fuse.js": "^7.0.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.14.10"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^18.2.7",
    "@angular/cli": "^18.2.7",
    "@angular/compiler-cli": "^18.2.0",
    "@types/chrome": "^0.0.277",
    "@types/jasmine": "~5.1.0",
    "autoprefixer": "^10.4.20",
    "concurrently": "^9.0.1",
    "esbuild": "^0.24.0",
    "jasmine-core": "~5.2.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "postcss": "^8.4.47",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.5.2"
  }
}
